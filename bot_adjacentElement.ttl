 bot:testadjacentElement
    a sh:NodeShape;
    sh:property[
        sh:path bot:adjacentElement;
        sh:name "Element Name";
        sh:description "The name of the Element.";
        sh:datatype xsd:string;
        sh:minCount 1;
        sh:disjoint bot:intersectingElement;
    ]